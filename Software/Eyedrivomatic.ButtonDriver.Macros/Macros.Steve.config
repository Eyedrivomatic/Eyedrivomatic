<?xml version="1.0" encoding="utf-8"?>
<Macros xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">

  <Macro DisplayName="Power" Icon="Images/Off.png">
    <Tasks>
      <ToggleRelay DisplayName="Cycle relay 1." Relay="1" Repeat="2" Delay="250" />
    </Tasks>
  </Macro>

  <Macro DisplayName="Mode">
    <Tasks>
      <ToggleRelay DisplayName="Cycle relay 2." Relay="2" Repeat="2" Delay="250" />
    </Tasks>
  </Macro>

  <Macro DisplayName="Spare">
    <Tasks>
      <ToggleRelay DisplayName="Cycle relay 3." Relay="3" Repeat="2" Delay="250" />
    </Tasks>
  </Macro>

  <Macro DisplayName="Tilt" Icon="Images/Tilt.png">
    <Tasks>
      <ToggleRelay DisplayName="Cycle relay 2." Relay="2" Repeat="2" Delay="250" />
      <Delay DisplayName="Wait for 500ms." Time="500" />
      <ToggleRelay DisplayName="Cycle relay 2." Relay="2" Repeat="2" Delay="250" />
      <Delay DisplayName="Wait for 500ms." Time="500" />
      <ToggleRelay DisplayName="Cycle relay 2." Relay="2" Repeat="2" Delay="250" />
      <Delay DisplayName="Wait for 500ms." Time="500" />
      <ToggleRelay DisplayName="Cycle relay 2." Relay="2" Repeat="2" Delay="250" />
    </Tasks>
  </Macro>

  <Macro DisplayName="Speed 2">
    <Tasks>
      <ToggleRelay DisplayName="Cycle relay 2." Relay="2" Repeat="2" Delay="250" />
      <Delay DisplayName="Wait for 500ms." Time="500" />
      <ToggleRelay DisplayName="Cycle relay 2." Relay="2" Repeat="2" Delay="250" />
      <Delay DisplayName="Wait for 500ms." Time="500" />
      <ToggleRelay DisplayName="Cycle relay 2 twice." Relay="2" Repeat="2" Delay="250" />
    </Tasks>
  </Macro>

</Macros>