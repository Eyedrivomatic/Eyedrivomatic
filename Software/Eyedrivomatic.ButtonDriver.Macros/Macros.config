  <Macro DisplayName="Turn Chair Off" IconPath="Images\Off.png">
    <Tasks>
      <ToggleRelay DisplayName="Toggle Relay 1 once." Relay="1" Repeat="1" Delay="0" />
    </Tasks>
  </Macro>
